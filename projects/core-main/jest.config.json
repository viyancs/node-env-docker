{
	"transform": {
		"^.+\\.js$": "babel-jest",
		"^.+\\.svelte$": [
			"svelte-jester",
			{
				"preprocess": true
			}
		]
	},
	"moduleFileExtensions": ["js", "svelte"],
	"setupFilesAfterEnv": ["@testing-library/jest-dom/extend-expect"],
	"extensionsToTreatAsEsm": [".svelte"],
	"testEnvironment": "./__test-utils__/custom-jest-environment.js"
}
